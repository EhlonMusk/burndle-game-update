{
  "name": "burndle-backend",
  "version": "1.0.0",
  "description": "Backend for BURNdle word guessing game with secure streak tracking and pump.fun integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test-scraper": "node test-scraper.js",
    "test-browser": "node -e \"const PumpFunScraper = require('./utils/pumpFunScraper'); const scraper = new PumpFunScraper(); scraper.getBrowserDataWithDebug().then(console.log).catch(console.error);\"",
    "install-browser": "npx puppeteer browsers install chrome",
    "update-puppeteer": "npm install puppeteer@latest"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bs58": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "playwright": "^1.55.0",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.7",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "wordle",
    "word-game",
    "blockchain",
    "solana",
    "streak-tracking",
    "pump.fun",
    "creator-rewards",
    "browser-automation",
    "puppeteer"
  ],
  "author": "Your Name",
  "license": "MIT"
}
